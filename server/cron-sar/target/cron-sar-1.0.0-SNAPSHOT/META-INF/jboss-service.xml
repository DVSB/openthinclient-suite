<?xml version='1.0' encoding='UTF-8' ?>

<!DOCTYPE server PUBLIC
   "-//JBoss//DTD MBean Service 3.2//EN"
   "http://www.jboss.org/j2ee/dtd/jboss-service_3_2.dtd"
>

<server>
  <loader-repository>tcat:loader=CronService
	<loader-repository-config>java2ParentDelegation=false </loader-repository-config>
  </loader-repository>
  
  <mbean code="org.openthinclient.cron.CronService" name="tcat:service=CronService"
    xmbean-dd="META-INF/cron-xmbean.xml">

    <depends>apacheds.system:service=Directory</depends>
    <depends>jboss.web:service=WebServer</depends>
  </mbean>
</server>
